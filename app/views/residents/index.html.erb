<body>
<p><h11><a href="/shelters">Shelter List</a></h11></p>
<p><h11><a href="/residents">List of All Residents</a></h11></p>


<header>
<h1>All Residents</h1>
</header>

<% @residents.each do |resident| %>
  <h3><a href="/residents/<%= resident.id %>"><%= resident.family_name %></a>
  <a href="/residents/<%= resident.id %>/edit">Edit Resident Record</a>
  </h3>
  <p>Long Term Housing Needed: <%=resident.long_term_housing_need%></p>
  <p>Family Size: <%=resident.family_size%></p>
  <p>Current Shelter ID: <%=resident.shelter_id%></p>
<% end %>

</body>


